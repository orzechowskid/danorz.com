{
  "env": {
    "test": {
      "presets": [[
        "@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }
      ]],
      "plugins": [[
        "@babel/plugin-syntax-dynamic-import"
      ], [
        "dynamic-import-node-babel-7"
      ]]
    }
  },
  "ignore": [
    "node_modules/**/*"
  ],
  "presets": [[
    "@babel/preset-env", {
      useBuiltIns: "usage"
    }
  ]]
}
