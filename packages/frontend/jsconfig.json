    "baseUrl": "./public",
    "checkJs": true,
    "jsx": "react",
    "lib": [ "dom", "ES2017" ],
    "moduleResolution": "node",
    "paths": {
      "~/*": [ "./*" ]
    },
    "reactNamespace": "createElement",
    "target": "ES6"
  },
  "exclude": [ "node_modules" ]
}
