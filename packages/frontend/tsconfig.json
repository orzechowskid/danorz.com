{
  "compilerOptions": {
    "baseUrl": "./public",
    "checkJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": [ "dom", "es2017" ],
    "moduleResolution": "node",
    "paths": {
      "~/*": [ "./*" ]
    },
    "reactNamespace": "createElement",
    "target": "es6"
  },
  "exclude": [ "node_modules" ]
}
